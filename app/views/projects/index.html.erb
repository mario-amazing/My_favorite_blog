<h1 id="page_title">Stuff I've Built</h1>

<p id="admin_button" >
  <% if user_signed_in? %>
    <%= link_to "New Project", new_project_path %>
  <% end %>
</p>

<div id="posts_wrapper" class="skinny_wrapper">
  <% @projects.each do |project| %>
    <div class="post">
      <p class="data"><%= project.created_at.strftime("%A, %b, %d") %></p>
      <h2><a href="#"><%= link_to project.title, project %></a></h3>
      <hr>
    </div>
  <% end %>
</div>
